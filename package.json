{
  "name": "recast",
  "version": "0.1.0",
  "description": "Programmatic identity system for generative logos and brand elements",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "next dev --turbo",
    "dev:webpack": "next dev",
    "dev:clean": "pnpm clean && next dev --turbo",
    "build": "tsup && next build",
    "build:lib": "tsup",
    "start": "next start",
    "clean": "rm -rf .next dist node_modules/.cache .turbopack",
    "dev:fresh": "pnpm clean && pnpm dev",
    "reset": "pnpm clean && pnpm install",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "vitest",
    "generate:logo": "pnpm build:lib && node scripts/generate-logo.js"
  },
  "keywords": [
    "generative",
    "logo",
    "identity",
    "brand",
    "programmatic",
    "design",
    "wave"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@base-ui-components/react": "1.0.0-beta.0",
    "@codemirror/lang-javascript": "^6.2.4",
    "@codemirror/theme-one-dark": "^6.1.3",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@uiw/react-codemirror": "^4.23.13",
    "canvas": "^2.11.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "file-saver": "^2.0.5",
    "gifencoder": "^2.0.1",
    "jszip": "^3.10.1",
    "lucide-react": "^0.523.0",
    "next": "15.3.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sharp": "^0.33.4",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "zustand": "^4.5.4"
  },
  "devDependencies": {
    "@types/file-saver": "^2.0.7",
    "@types/gifencoder": "^2.0.1",
    "@types/node": "^20.11.5",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.56.0",
    "postcss": "^8.4.39",
    "tailwindcss": "^3.4.4",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "engines": {
    "node": ">=18"
  }
}